<witos>
	<append xpath="/Sounds">	
		<SoundDataNode name="Ballon_start"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
			<Noise ID="2" noise="11" time="3" muffled_when_crouched="0.5"/>
			<AudioClip ClipName="#@modfolder:Resources/ballonsound.unity3d?shipon"/>
			<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> 
			<MaxVoices value="10"/> <MaxRepeatRate value="0.01"/> </SoundDataNode>
		<SoundDataNode name="Ballon_stop"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
			<Noise ID="2" noise="11" time="3" muffled_when_crouched="0.5"/>
			<AudioClip ClipName="#@modfolder:Resources/ballonsound.unity3d?poff"/>
			<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="10"/> <MaxRepeatRate value="0.01"/> </SoundDataNode>
		<SoundDataNode name="Ballon_flying"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
			<AudioClip ClipName="#@modfolder:Resources/ballonsound.unity3d?airflyin" Loop="true"/>
			<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="10"/> <MaxRepeatRate value="0.01"/> </SoundDataNode>
				
	</append>
</witos>